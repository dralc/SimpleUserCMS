{
  "name": "simpleusercms",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "npm run start-graphql",
    "start-rest": "node ./bin/www",
    "start-graphql": "APP=appGraphql node ./bin/www",
    "start-redis": "$REDIS_DIR/src/redis-server --daemonize yes",
    "start-rediscli": "$REDIS_DIR/src/redis-cli",
    "dev": "DEBUG=sim npm run start-redis && nodemon npm start",
    "test-integration": "DEBUG=sim npx ava --fail-fast --verbose",
    "seed-redis": "node ./mock/seedRedis 500"
  },
  "dependencies": {
    "apollo-server-express": "^2.11.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "ioredis": "^4.16.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "ava": "^3.5.0",
    "faker": "^4.1.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "test-listen": "^1.1.0"
  }
}
